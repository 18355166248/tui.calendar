{
  "name": "@toast-ui/calendar",
  "author": "NHN FE Development Lab <dl_javascript@nhn.com>",
  "version": "2.0.0-alpha",
  "main": "./dist/toastui-calendar.js",
  "types": "./types/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "module": "./dist/esm/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/toastui-calendar.js"
    },
    "./ie11": {
      "import": "./dist/toastui-calendar.ie11.js",
      "require": "./dist/toastui-calendar.ie11.js"
    },
    "./toastui-calendar.css": "./dist/toastui-calendar.css",
    "./toastui-calendar.min.css": "./dist/toastui-calendar.min.css"
  },
  "license": "MIT",
  "description": "TOAST UI Calendar",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhn/tui.calendar.git"
  },
  "keywords": [
    "nhn",
    "toast",
    "toastui",
    "toast-ui",
    "calendar",
    "fullcalendar",
    "daily",
    "weekly",
    "monthly",
    "business week",
    "milestone",
    "task",
    "allday"
  ],
  "files": [
    "dist",
    "index.d.ts",
    "types"
  ],
  "dependencies": {
    "tui-date-picker": "^4.0.1",
    "tui-time-picker": "^2.0.1"
  },
  "scripts": {
    "check-types": "tsc --project ./tsconfig.json --noEmit",
    "validate": "npm run check-types && npm run lint",
    "lint": "eslint .",
    "cpy-dist2doc": "mkdir -p doc/dist && cp -f -r dist doc",
    "release-note": "tuie",
    "build-all": "rm -rf dist && concurrently 'npm:build:*'",
    "build:modern": "webpack --config webpack.config.js && webpack --config webpack.config.js --env minify",
    "build:ie11": "webpack --config webpack.config.js --env ie11 && webpack --config webpack.config.js --env minify ie11",
    "build:esm": "node scripts/build-esm.js",
    "analyze": "webpack --config webpack.config.js --env --profile --json > stats.json && webpack-bundle-analyzer stats.json ./dist",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:build:docs": "STORYBOOK_ENV=docs build-storybook",
    "ts2js": "tsc --outDir tmpdoc --sourceMap false --target ES2015 --noEmit false",
    "doc:dev": "npm run build:prod && npm run ts2js && tuidoc --serv",
    "doc": "npm run ts2js && tuidoc"
  }
}
